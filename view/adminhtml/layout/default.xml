<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Wm21w_AdminTheme::css/css.css"/>
    </head>
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_image_src" xsi:type="string">/images/vitcas-logo.png</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="copyright">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Wm21w_AdminTheme::page/copyright.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>